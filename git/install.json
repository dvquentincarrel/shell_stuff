{
    "dependencies": ["git", "grep", "awk", "tail", "cut"],
    "installation": {
        "config": {
            "dir": "$HOME/.config/git",
            "files": ["config", "gitignore"]
        },
        "setup": {
            "dir": "$HOME/.config/bash/setup",
            "renamed_files": [
              { "src": "autocomplete.bash", "dest": "999_git_alias_autocomplete.bash" },
              { "src": "aliases.bash", "dest": "100_git_aliases.bash" }
            ]
        }
    }
}
